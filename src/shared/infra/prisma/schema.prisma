// ◮◮◮ GENERATED BY AURORA ◮◮◮
datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}

generator client {
  provider = "prisma-client-js"
}

model Seller {
  id           String     @id @default(uuid())
  supervisor   Supervisor @relation(fields: [supervisorId], references: [id])
  supervisorId String
  image        String
  name         String
  email        String     @unique
  created_at   DateTime   @default(now())
}

model Supervisor {
  id         String   @id @default(uuid())
  image      String
  name       String
  email      String
  created_at DateTime @default(now())
  seller     Seller[]
}