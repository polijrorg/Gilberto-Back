model Supervisor {
  id           String     @id @default(uuid())
  image        String
  name         String
  email        String
  seller       Seller[]
  created_at  DateTime   @default(now())
}

model Seller {
  id           String     @id @default(uuid())
  supervisor   Supervisor @relation(fields: [supervisorId], references: [id])
  supervisorId String
}
